#include "libft.h"
#include <stdio.h>

int		ft_strncmp(const char *s1, const char *s2, size_t n)
{
	size_t	i;

	i = 0;
	if (!n)
		return (0);
	while (i < n - 1 && s1[i] == s2[i] && s1[i] && s2[i])
		i++;
	printf("s1 : %c\ns2 : %c\n", s1[i], s2[i]);
	return ((unsigned char)s1[i] - (unsigned char)s2[i]);
}
